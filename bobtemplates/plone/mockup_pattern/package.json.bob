    "@types/sinon": "^10.0.11",
    "babel-loader": "^8.2.4",
    "clean-css-cli": "^5.6.0",
    "copy-webpack-plugin": "^10.2.4",
    "copyfiles": "^2.4.1",
    "core-js": "3.21.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "expose-loader": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^3.1.0",
    "inspectpack": "^4.7.1",
    "jest": "^27.5.1",
    "jest-raw-loader": "^1.0.1",
    "jest-watch-typeahead": "^1.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prism-themes": "^1.9.0",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.3.1",
    "timezone-mock": "^1.3.1",
    "webpack": "^5.71.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.0",
    "webpack-modernizr-loader": "^5.0.0",
    "whybundled": "^2.0.0",
    "yarn": "^1.22.18"
  },
  "scripts": {
    "build": "npm-run-all build:webpack build:css",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "build:css": "npm-run-all css-compile-main css-minify-main",
    "watch:webpack": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "watch:scss": "nodemon --watch resources/scss/ --ext scss --exec \"yarn build:css\"",
    "css-main": "npm-run-all css-compile-main css-minify-main",
    "css-compile-main": "sass --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css resources/patterns/scss/:src/{{{ package.namespace }}}/{{{ package.name }}}/browser/static/patterns/",
    "css-minify-main": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output src/{{{ package.namespace }}}/{{{ package.name }}}/browser/static/patterns/{{{ package.name }}}.min.css src/{{{ package.namespace }}}/{{{ package.name }}}/browser/static/patterns/*.css",
    "css-lint": "stylelint \"resources/scss/*.scss\" --cache --cache-location .cache/.stylelintcache"
  },
  "dependencies": {
    "@patternslib/patternslib": "^8.0.0",
    "@plone/mockup": "^5.0.0-alpha.1",
    "bootstrap": "^5.1.3"
  }
}
